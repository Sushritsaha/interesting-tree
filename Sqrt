class Sqrt {
    // TC -> Log N where N is the given number
    // SC -> Order of 1
    public int mySqrt(int x) {
        int left = 1, right = x;
        while(left <= right){
            int mid = left + (right-left)/2;  //(left+right)/2
            if(mid<x/mid) left = mid+1;
            else if(mid>x/mid) right = mid-1;
            else return mid;
        }
        return right;
    }
}
